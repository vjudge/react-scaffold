{
  "name": "create-react-typescript",
  "description": "react typescript base",
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "axios": "^0.26.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "scripts": {
    "start": "run-p --race start:server start:app",
    "start:app": "react-scripts start",
    "start:server": "node --experimental-loader ts-node/esm ./server/server.ts && wait-on tcp:5000",
    "build": "react-scripts build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "eslint": "tslint -c tslint.json -p tslint-tsconfig.json --fix",
    "prettier": "prettier --write '**/*.{tsx,ts,jsx,js,json}'"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@cloudflare/workers-types": "^3.4.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.11",
    "@types/express": "^4.16.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/webpack-env": "^1.16.3",
    "babel-jest": "^27.5.1",
    "body-parser": "^1.18.3",
    "commitizen": "^3.0.5",
    "concurrently": "^4.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^8.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.4",
    "react-scripts": "^2.1.5",
    "semantic-release": "^15.13.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^3.3.3",
    "wait-on": "^3.2.0"
  },
  "proxy": "http://localhost:8888",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --color",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint -c tslint.json -p tslint-tsconfig.json",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
